    "redux-logger": "^2.7.0",
    "redux-thunk": "^2.1.0",
    "sass-loader": "^4.1.1",
    "sequelize": "^3.24.6",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.3"
  },
  "devDependencies": {
    "chai": "^3.5.0",
