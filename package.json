{
  "name": "go-outside",
  "version": "0.0.1",
  "description": "A small app to preserve your sanity during the workday.",
  "main": "index.js",
  "scripts": {
    "test": "check-node-version --node '>= 6.7.0' && bin/setup && mocha --compilers js:babel-register tests/**/*.test.js tests/**/*.test.jsx",
    "test-watch": "check-node-version --node '>= 6.7.0' && bin/setup && mocha --compilers js:babel-register --watch tests/**/*.test.js tests/**/*.test.jsx",
    "build": "check-node-version --node '>= 6.7.0' && bin/setup && webpack",
    "build-watch": "check-node-version --node '>= 6.7.0' && bin/setup && webpack -w",
    "start": "check-node-version --node '>= 6.7.0' && bin/setup && nodemon server/start.js",
    "seed": "node db/seed.js",
    "postinstall": "webpack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dgodow/go-outside.git"
  },
  "keywords": [
    "react",
    "redux",
    "skeleton"
  ],
  "author": "Brian Nichols, Milo Kang, Colin Jaffe, and David Godow",  
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dgodow/go-outside/issues"
  },
  "homepage": "https://github.com/dgodow/go-outside#readme",
  "dependencies": {
    "axios": "^0.15.2",
    "babel": "^6.5.2",
    "babel-core": "^6.18.0",
    "babel-loader": "^6.2.7",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.18.0",
    "bcrypt": "^0.8.7",
    "body-parser": "^1.15.2",
    "bootstrap": "^3.3.7",
    "chai-enzyme": "^0.5.2",
    "chalk": "^1.1.3",
    "check-node-version": "^1.1.2",
    "cookie-session": "^2.0.0-alpha.1",
    "css-loader": "^0.26.1",
    "enzyme": "^2.5.1",
    "express": "^4.14.0",
    "express-session": "^1.14.2",
    "faker": "^3.1.0",
    "font-awesome": "^4.7.0",
    "lodash": "^4.17.4",
    "moment": "^2.17.1",
    "node-sass": "^4.3.0",
    "nodemon": "^1.11.0",
    "passport": "^0.3.2",
    "passport-facebook": "^2.1.1",
